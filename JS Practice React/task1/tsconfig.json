{
    "include": ["src"],
    "compilerOptions": {
      "target" : "es5",
      "module" : "commonjs",
      "lib": ["dom", "es2015", "es2016"],
      "allowJs": true,
      "checkJs": false,
      "jsx": "react",
      "strict": true,
      "noImplicitAny": false,
      "strictNullChecks": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["./types.d.ts", "node"]
    }
  }